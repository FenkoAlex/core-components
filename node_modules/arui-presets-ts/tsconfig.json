{
    "compilerOptions": {
        "lib": [
            "dom",
            "es2015",
            "es2016"
        ],
        "experimentalDecorators": true,
        "jsx": "react",
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "sourceMap": true,
        "allowJs": true
    }
}
